my_list=list(map(str,input().split(",")))
choice=int(input("enter the choice"))
if(choice==1):
    print("append")
elif(choice==2):
    print("pop")
elif(choice==3):
    print("sort")
elif(choice==4):
    print("length")
else:
    print("good morning")
