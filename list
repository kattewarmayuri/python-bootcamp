my_list=[1,8,3,7,9]
my_list.append(9999)#adding 9999 at end
my_list.insert(0,44)#insert 44 in index 0
print(len(my_list))
my_list.pop(2)#pops the element
my_list_2=[7,9,6,9]#adding list
new_list=my_list+my_list_2
new_list=my_list.copy()
my_list.pop(2)#removing 2index
cnt=my_list.count(3)
print(cnt)
my_list.sort()
print(*my_list) 
print(*new_list)#uset to remove comma
